{% extends 'base.html.twig' %}
{% block body %}
    <h1>Liste des articles</h1>
    <table class="table">
        <tr>
            <th>Identifiant</th>
            <th>Libellé</th>
            <th>Quantité</th>
            <th>Gamme</th>
        </tr>

    {% for tab in articles  %}
        <tr>
        {% for value in tab %}
           <td>{{ value }}</td>
        {% endfor %}
        </tr>
    {% endfor %}
    </table>

{% endblock %}